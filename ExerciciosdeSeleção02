// Faça um algoritmo que leia três números inteiros e mostre o menor deles.
package main

import "fmt"

func main() {
	var n1 int
	var n2 int
	var n3 int
	fmt.Print("Digite o primeiro número: ")
	fmt.Scanln(&n1)
	fmt.Print("Digite o segundo número: ")
	fmt.Scanln(&n2)
	fmt.Print("Digite o terceiro número: ")
	fmt.Scanln(&n3)
	menor := n1
	if n2 < menor {
		menor = n2
	} else if n3 < menor {
		menor = n3
	}
	fmt.Println("O menor número é: ", menor)
}