// Faça um algoritmo que leia o preço de um produto e mostre o seu valor com desconto de 10%.
package main

import "fmt"

func main() {
	var preço float64
	fmt.Print("O preço do produto é: ")
	fmt.Scan(&preço)
	valor_com_desconto := preço * 0.9
	fmt.Printf("Novo valor com desconto será: %.2f\n", valor_com_desconto)
}
